<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- squadron -->
<mapper namespace="com.henu.competition.mapper.SquadronMapper">
    <!-- 字段映射 -->
    <resultMap id="squadronMap" type="com.henu.competition.model.Squadron"/>

    <!-- This code was generated by TableGo tools, mark 1 begin. -->
    <!-- 表所有字段 -->
    <sql id="allColumns">
        s.id, s.name, s.projectId, s.user_id, s.created_by, s.creation_date, s.last_updated_by, s.last_update_date
    </sql>
    <!-- This code was generated by TableGo tools, mark 1 end. -->

    <!-- 分页查询列表 -->
    <select id="findSquadronList" resultMap="squadronMap">
        SELECT
            <include refid="allColumns" />
        FROM squadron s WHERE 1 = 1
        ORDER BY s.CREATION_DATE DESC
    </select>

    <!-- 查询 -->
    <select id="getSquadron" resultMap="squadronMap">
        SELECT
            <include refid="allColumns" />
        FROM squadron s WHERE 1 = 1
    </select>

</mapper>