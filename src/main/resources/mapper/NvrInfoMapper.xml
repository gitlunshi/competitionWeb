<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 录像机信息(nvr_info) -->
<mapper namespace="com.henu.competition.mapper.NvrInfoMapper">
    <!-- 字段映射 -->
    <resultMap id="nvrInfoMap" type="com.henu.competition.model.NvrInfo"/>

    <!-- This code was generated by TableGo tools, mark 1 begin. -->
    <!-- 表所有字段 -->
    <sql id="allColumns">
        ni.id, ni.ip_address, ni.server_port, ni.login_name, ni.login_password, ni.stadium_id, ni.channel_num, ni.status,
        ni.created_by, ni.creation_date, ni.last_updated_by, ni.last_update_date
    </sql>
    <!-- This code was generated by TableGo tools, mark 1 end. -->

    <!-- 分页查询录像机信息列表 -->
    <select id="findNvrInfoList" resultMap="nvrInfoMap">
        SELECT
            <include refid="allColumns" />
        FROM nvr_info ni WHERE 1 = 1
        ORDER BY ni.CREATION_DATE DESC
    </select>

    <!-- 查询录像机信息 -->
    <select id="getNvrInfo" resultMap="nvrInfoMap">
        SELECT
            <include refid="allColumns" />
        FROM nvr_info ni WHERE 1 = 1
    </select>

</mapper>